{
  "name": "storefront-backend-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon",
    "docker-up": "docker compose up -d",
    "docker-down": "docker compose down",
    "test:jasmine": "jasmine-ts --config=spec/support/jasmine.json",
    "test:dev": "jest --watchAll --coverage",
    "test": "ENV=test db-migrate up -e test --config database.json && jasmine-ts --config=spec/support/jasmine.json && db-migrate --env test reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeff-eng/storefront-backend-api.git"
  },
  "keywords": [],
  "author": "",
