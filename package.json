  "bugs": {
    "url": "https://github.com/jeff-eng/storefront-backend-api/issues"
  },
  "homepage": "https://github.com/jeff-eng/storefront-backend-api#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
