{
    "verbose": true,
    "automock": true,
    "preset": "ts-jest",
    "roots": ["<rootDir>/src"],
    "transform": {
        "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "./src/.*\\.(test|spec)?\\.(ts|ts)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}